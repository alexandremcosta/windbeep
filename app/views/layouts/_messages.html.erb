<% flash.each do |key, value| %>
  <div class="alert alert-dismissible fade show alert-<%= key %>">
    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
      <span aria-hidden="true">&times;</span>
    </button>

    <ul>
      <li>
        <%= value %>
      </li>
    </ul>
  </div>
<% end %>
